{
    "name": "swooletw/hyperf-support",
    "description": "The Support package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "support",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "hyperf/context": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/tappable": "~3.1.0",
        "hyperf/collection": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Support\\": "src/"
        },
        "files": [
            "src/Functions.php",
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}